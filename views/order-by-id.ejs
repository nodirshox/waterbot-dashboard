<%- include('includes/header') %>
      
<div class="col-md-12">
  <div class="card card-default">
    <div class="card-header">  <a href="/order" class="btn-default btn">Buyurtmalar</a></div>
    <div class="card-body">
      <b>Kim:</b> <%= cat.firstname %><br />
      <b>Telefon:</b> <%= cat.phone %><br />
      <b>Izoh:</b> <%= cat.comment %><br />
      <b>Buyurtma:</b> <%= cat.order %><br />
      <b>Registratsiya payti:</b> <%= time %><br />
      <b>Yetkazib berish vaqti:</b> <%= cat.orderdate %>, <%= cat.ordertime %><br />
      <b>Manzil:</b> <%= cat.location %><br />
      <b>Yetkazildi:</b> <%= cat.delivery_person %><br />
      <b>Buyurtma holati:</b> <%if (cat.status === 1) { %>
        🚕 Buyurtma buyurtmachilarga berilgan
        <% } else if(cat.status === 2) { %>
        ✅ Buyurtma yetkazildi
        <% }else if(cat.status === 3) { %>
          🕔 Buyurtma kelib tushgan
          <% } else if(cat.status === 0) { %>
        ❌ Buyurtma yetkazilmadi
        
        <% } %>
        <br />
        <a href="/edit-order/<%= cat._id %>" class="btn-primary btn">O'zgartirish</a>
    </div>
  </div>
</div>

<%- include('includes/footer') %>